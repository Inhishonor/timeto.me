CREATE TABLE EventSQ (
    id INTEGER AS Int NOT NULL PRIMARY KEY,
    text TEXT NOT NULL,
    utc_time INTEGER AS Int NOT NULL
);

getAscByTime:
SELECT *
FROM EventSQ
ORDER BY utc_time ASC;

insertObject:
INSERT INTO EventSQ
VALUES ?;

deleteById:
DELETE FROM EventSQ
WHERE id = ?;

updateById:
UPDATE EventSQ
SET text = ?, utc_time = ?
WHERE id = ?;

truncate:
DELETE FROM EventSQ;
